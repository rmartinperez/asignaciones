<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/public/css/bootstrap.min.css') }}" />
        <link rel="stylesheets" href="{{ asset('bundles/public/css/style.css') }}" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
            {{ include('menu.html.twig')}}
        {% endblock %}
        {% block javascripts %}
        <script src="{{ asset('bundles/public/js/jquery-1.12.0.min.js') }}"></script>
        <script src="{{ asset('bundles/public/js/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>
